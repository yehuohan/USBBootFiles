find --set-root /BOOT/GRUB/submenu.lst

title Return \n
configfile ()/boot/grub/submenu.lst

# 命令行安装
title Install kali
find --set-root /install/vmlinuz
kernel /install/vmlinuz mtrr vga=788
initrd /install/initrd.gz

# 图形界面方式安装
title Installgui kali \n
find --set-root /install/gtk/vmlinuz
kernel /install/gtk/vmlinuz video=vesa:ywrap,mtrr vga=788
initrd /install/gtk/initrd.gz

# u盘运行kali
title Start kali live \n
find --set-root /live/vmlinuz
kernel /live/vmlinuz boot=live username=root hostname=kali
initrd /live/initrd.img




